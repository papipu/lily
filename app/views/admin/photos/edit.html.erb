<% form_for @photo, :html => { :multipart => true }, :url => { :action => "update", :complect=>@complect.id } do |f| %>
  <p><%= f.error_messages %></p>
  <p>
    <label for="title">название</label>
    <%= f.text_field :title %>
  </p>
  <p>
    <label for="image">фотография</label>
    <%= f.file_field :image %>
  </p>
  <p><%= f.submit 'изменить' %></p>
<% end %>